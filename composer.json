{
    "name": "mrshennawy/committer",
    "description": "Manage Git commits effortlessly with the 'Committer'.",
    "keywords": ["committer"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mahmoud Shennawy",
            "email": "m.alshenaawy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.1",
        "symfony/console": "^4.0|^5.0|^6.0",
        "symfony/process": "^4.2|^5.0|^6.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "bin": [
        "bin/gc"
    ],
    "autoload": {
        "psr-4": {
            "MrShennawy\\Committer\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
